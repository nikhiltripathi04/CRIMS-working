{
  "expo": {
    "name": "ConERP",
    "slug": "frontend",
    "version": "2.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/ConErp logo1.png",
    "scheme": "frontend",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.feathrtech.ConERP",
      "buildNumber": "2.0.1",
      "infoPlist": {
        "NSCameraUsageDescription": "This app requires camera access to allow you to take and upload photos.",
        "NSPhotoLibraryUsageDescription": "This app requires access to your photo library to let you choose and upload photos.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ConErp logo1.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.feathrtech.ConERP",
      "versionCode": 3
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/ConErp logo1.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/ConErp logo1.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "minSdkVersion": 24
          }
        }
      ],
      "expo-video",
      "expo-font",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos",
          "saveToPhotosPermission": "Allow $(PRODUCT_NAME) to save photos to your gallery",
          "isAccessMediaLocationEnabled": true
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "56958306-3220-4723-ad86-030a530ebdf8"
      }
    }
  }
}